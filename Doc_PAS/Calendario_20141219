.menuButtonBlau a.exit {
    background: url(../images/icons/logout.png) 0px 2px no-repeat;
    background-size: 20px Auto;
    float: left;
    
    margin-right:-10px;
    color: #007BBF;
    margin-top: -9px;
    /* height: 33px; */
    padding: 4px 15px 5px 24px;
}


